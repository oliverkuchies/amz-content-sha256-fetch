{
	"name": "amz-content-sha256-fetch",
	"version": "1.0.9",
	"description": "Utilise amz-content-sha256 header automatically inferred from content in fetch",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"test": "vitest",
		"lint": "biome check ./src",
		"lint:fix": "biome check ./src --fix --unsafe",
