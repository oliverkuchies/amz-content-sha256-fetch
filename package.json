	"license": "ISC",
	"bugs": {
		"url": "https://github.com/oliverkuchies/amz-content-sha256-fetch/issues"
	},
	"homepage": "https://github.com/oliverkuchies/amz-content-sha256-fetch#readme",
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"globals": "^16.0.0",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.26.1",
		"vitest": "^3.0.8"
	},
	"dependencies": {
		"js-sha256": "^0.11.0",
		"vitest-fetch-mock": "^0.4.5"
	}
